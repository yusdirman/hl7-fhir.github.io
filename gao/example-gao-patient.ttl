@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# - resource -------------------------------------------------------------------

<http://hl7.org/fhir/Patientexample-gao-patient> a fhir:Patient;
  fhir:nodeRole fhir:treeRoot;
  fhir:Resource.id [ fhir:value "example-gao-patient"];
  fhir:Resource.meta [
     fhir:Meta.profile [
       fhir:value "http://hl7.org/fhir/StructureDefinition/gao-patient";
       fhir:index 0
     ]
  ];
  fhir:DomainResource.text [
     fhir:Narrative.status [ fhir:value "generated" ];
     fhir:Narrative.div "<div xmlns=\"http://www.w3.org/1999/xhtml\">\n        <p>A 50-year-old male.</p>\n      </div>"
  ];
  fhir:Patient.gender [ fhir:value "male"];
  fhir:Patient.birthDate [ fhir:value "1965-11-20"^^xsd:date].

